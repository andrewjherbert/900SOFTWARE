c tape 1
at ptr file ../../MASTERS/ALDENHAM/EDIT8PSRC1.900
j 8
st 6927
c  tape 2, n.b. has two parts
at ptr file ../../MASTERS/ALDENHAM/EDIT8PSRC2.900
j 9
st 6927
j 9
st 6927
c load unidump-d
select out auto
nonstop on
at ptr file ../../MASTERS/ALDENHAM/UNIDUMP-D.BIN
at ptp file EDIT8P.BIN
at tty inline
3
0
33
32
3453
<! halt !><!!>
j 8181
