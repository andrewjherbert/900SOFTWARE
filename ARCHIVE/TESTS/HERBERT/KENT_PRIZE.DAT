C Kent Programming Prize Competition 1972


c **************************

C Demonstrate Update and Costing Program
C A.J. HERBERT 27 September 2020

C ../../SCRIPTS/HERBERT fails to build binaries, when fixed need to check this script to run demos.

c **************************

on 905 16K
nonstop on

C Update program
at ptr file ../../SCRIPTS/HERBERT/KENT_UPDATE.BIN
j 8181
st 8164

at tty inline
R<!!>

j 16
c test run with bad data

c call for update tape
at ptr inline
AB 1111   2.00
B3 3333   5.00
DD 888E   6.00
AB 2222  -3.50
BB 3333   4.00
** 9999   9.57
[[ 9999   0.00
<!!>


c test run with clean data

c call for update tape
at ptr inline
AB 1111   6.00
BC 3333   5.00
DD 8888   4.00
AB 2222   3.50
BB 3333   2.00
ZZ 9999   9.57
[[ 9999   0.00
<!!>
at tty inline
MC<!!>
r

c call for master tape
at ptp file kent_master.txt
at ptr inline
AA 0000   1.00
AB 1111   1.50
AB 1155   3.55
AB 1175   3.75
EE 8888   9.00
ZZ 9999   0.25
[[ 9999   0.00
<!!>
at tty inline
C<!!>
r

de ptp
print kent_master.txt
sh t

C Costing program

at ptr file ../../SCRIPTS/HERBERT/KENT_COSTING.BIN
j 8181

j 16
c call for component tape
at ptr file kent_master.txt
at tty inline
C<!!>
r

c call for ASSEMBLY tape
at ptr inline
ASSEMBLY NO. 98J55
NO.       QTY
AA 0000     5
AB 1111     2
AB 1155    10
AB 1175     3
[[ 9999     0

<!!>
at tty inline
C<!!>
r

sh t

c reader unloads, run with bad data
at tty inline
M<!!>
r

c call for component tape
at ptr file kent_master.txt
at tty inline
C<!!>
r

c call for ASSEMBLY tape
at ptr inline
ASSEMBLY NO. 98J55
NO.       QTY
AA 0000     5
AB 1111    -2
** 1155    10
DD 1175    -3
[[ 9999     0
<!!>
at tty inline
C<!!>
r

delete kent_master.txt
sh t
