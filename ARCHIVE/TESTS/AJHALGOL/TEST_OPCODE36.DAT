c Algol function codes: procedure call with real array by name


on 903 16K

c translate trivial program
at ptr file ../../SCRIPTS/AJHALGOL/ALG1(AJH).BIN
j 8181

at ptr inline
opcode;
"begin" 
    "procedure" p(r); "real" "array" r; r[1] := r[1] + 10.0;
    "real" "array" s[1:10];
    s[1] := 20.0; p(s); "print" s[1]        
"end"
<! halt !><!!>
at ptp file TEST.RLB
j 8
st 9
de ptp

printalgol TEST.RLB

c load new LP system
at ptr file ../../SCRIPTS/AJHALGOL/ALG16KLP(AJH).BIN
j 8181

c run trivial program
at ptr file TEST.RLB
at tty inline
R
C
<!!>
nonstop on
select out tty
nonprinting off
j 8
c runs off tty
nonstop off

delete TEST.RLB
sh t