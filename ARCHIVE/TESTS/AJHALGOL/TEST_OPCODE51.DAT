c Algol function codes: procedure call switch to switch

on 903 16K

c translate trivial program
at ptr file ../../SCRIPTS/AJHALGOL/ALG1(AJH).BIN
j 8181

at ptr inline
opcode;
"begin" 
    "switch" s := l;
    "procedure" p1(s); "switch" s; "goto" s[1]; 
    "procedure" p2(s); "switch" s; p1(s);
    p2(s);
l:  "print" {L}      
"end"
<! halt !><!!>
at ptp file TEST.RLB
j 8
st 9
de ptp

printalgol TEST.RLB

c load new LP system
at ptr file ../../SCRIPTS/AJHALGOL/ALG16KLP(AJH).BIN
j 8181

c run trivial program
at ptr file TEST.RLB
at tty inline
R
C
<!!>
nonstop on
select out tty
nonprinting off
j 8
c runs off tty
nonstop off

delete TEST.RLB
sh t

