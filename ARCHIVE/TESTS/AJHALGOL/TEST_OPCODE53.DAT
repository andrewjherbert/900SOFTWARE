c Algol function codes: procedure call function to function

on 903 16K

c translate trivial program
at ptr file ../../SCRIPTS/AJHALGOL/ALG1(AJH).BIN
j 8181

at ptr inline
opcode;
"begin" 
    "integer" "procedure" p1; p1 := 1;
    "integer" "procedure" p2(p); "integer" "procedure" p; p2 := p; 
    "print" p2(p1)      
"end"
<! halt !><!!>
at ptp file TEST.RLB
j 8
st 9
de ptp

printalgol TEST.RLB

c load new LP system
at ptr file ../../SCRIPTS/AJHALGOL/ALG16KLP(AJH).BIN
j 8181

c run trivial program
at ptr file TEST.RLB
at tty inline
R
C
<!!>
nonstop on
select out tty
nonprinting off
j 8
c runs off tty
nonstop off

delete TEST.RLB
sh t

