C Demonstrate WORKSHOP in offline mode

on 903
c Run with unpatched WORKSHOP
at ptr file ../../MASTERS/WORKSHOP/ONLINE.BIN
at tty inline

\AJH001

WHERE A=2
WHERE D=0
CALCULATE B=QATN(QEXP(QLNE(QSQT(A*A))))
ITERATE A=QSIN(B)/QCOS(B)
ITERATE D=D+1
PRINT D;A
RUN
RUN
RUN
RUN
&
<! halt !><!!>
j 8181
st 2481

