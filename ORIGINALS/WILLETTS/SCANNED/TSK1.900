           WRITE PRINT-REC FROM LINE3 AFTER ONE
           WRITE PRINT-REC FROM LINE4 AFTER ONE
           WRITE PRINT REC FROM LINE5 AFTER ONE.
           MOVE SPACES TO PRINT-REC.
           WRITE PRINT-REC AFTER ONE
           WRITE PRINT-REC AFTER ONE
           WRITE PRINT-REC AFTER ONE.
           GO TO READ-PARA.
