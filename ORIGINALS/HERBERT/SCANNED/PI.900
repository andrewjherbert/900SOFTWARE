C     DIVIDE
      SUBROUTINE HDVD
      DIMENSION IA(65),IB(65)
      COMMON ISCAL,LENG,IA,IB
      REM=0.0
      SCAL=ISCAL
      DO 300 L=1,LENG
      W=100000.0*REM+FLOAT(IA(L))
      IQUOT=W/SCAL
      REM=W-FLAT(IQUOT)*SCAL
      IA(L)=IQUOT
  300 CONTINUE
      RETURN
      END
C     SUBTRACT
      SUBROUTINE HSUB
      DIMENSION IA(65),IB(65)
      COMMON ISCAL,LENG,IA,IB
      IBOR=0
      DO 300 K=1,LENG
      L=LENG-K+1
      J=IA(L)-IB(L)-IBOR
      IF(J) 200,210,210
  200 IBOR=1
      J=J+100000
      GOTO 220
  210 IBOR=0
  220 IA(L)=J
  300 CONTINUE
      RETURN
      END
