      GLOBAL,HATAN,HMPY,HDVD]
C     ARCTAN
      SUBROUTINE HATAN(N,NTERM)
      DIMENSION IA(65),IB(65)
      COMMON ISCAL,LENG,IA,IB
      DO 300 L=2,LENG
      IA(L)=99999
  300 CONTINUE
      IA(1)=0
      DO 500 K=1,NTERM
      L=NTERM-K+1
      ISCAL=2*L-1
      CALL HMPY
      ISCAL=2*L+1
